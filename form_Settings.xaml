<Window x:Class="Free_Gamma.form_Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Free_Gamma"
        mc:Ignorable="d"
        Title="Settings" Height="350" Width="500" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">

    <DockPanel x:Name="pnl_1" Margin="30">

        <Grid DockPanel.Dock="Bottom">
            <Button x:Name="btn_Close" Content="Close" Padding="30,5" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Grid>

        <StackPanel >
            
            <GroupBox VerticalAlignment="Top" Padding="10,10,10,5">
                <CheckBox x:Name="chk_StartWithWindows"  Content="Start at Windows startup" />
            </GroupBox>
            
        </StackPanel>
       
        
    </DockPanel>
</Window>
